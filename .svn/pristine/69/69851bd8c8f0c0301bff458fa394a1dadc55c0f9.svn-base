.stv-container {
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.withAnimate {
  transition: all 100ms ease;
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  -webkit-backface-visibility: hidden;
  -moz-backface-visibility: hidden;
  -ms-backface-visibility: hidden;
  backface-visibility: hidden;
  -webkit-perspective: 1000;
  -moz-perspective: 1000;
  -ms-perspective: 1000;
  perspective: 1000;
}
.stv-container .tab-bar {
  position: fixed;
  display: flex;
  font-size: 30rpx;
  background: #03A9F4;
  /*margin-bottom:10px;*/
}
.stv-container .tab-bar .tab-active {
  color: #fff;
}
.stv-container .tab-bar .tab {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-top: 16rpx;
  padding-bottom: 20rpx;
}

.stv-container .scroll-view {
  position: relative;
  width: 100%;
  height: 100%;
  background: #e7eaef;
  margin-top:50px;
}
.stv-container .scroll-view .scroll-view-wrapper {
  position: absolute;
  top: 0;
  bottom: 0;
  display: flex;
}
.stv-container .scroll-view .scroll-view-wrapper .one-scene {
  height: 100%;
}

page {
  font-family: Helvetica,
                 Arial,
                 "Hiragino Sans GB",
                 "Source Han Sans CN",
                 "PingFang SC",
                 Roboto,
                 "微软雅黑",
                 "Heiti SC",
                 "Microsoft Yahei",
                 sans-serif;
  background: #03A9F4
}


.swiper-tab {
  height: 45px;
  line-height: 45px;
  background: #fff;
  display: flex;
  border-top: 1px solid #0292F1;
  position: fixed;
  z-index: 2;
  bottom:0;
  left: 0;
  width: 100%;
}

.swiper-tab-list {
  /*margin: 0 20px;
  padding: 0 4px; */
  text-align: center;
  border-right: 1px solid #0292F1;
  width: 50%;
  color: #fff;
}

.on {
  color: #fff;
}

.swiper-box {
  height: 100%;
  width: 100%;
  overflow: hidden;
  margin-top: 48px;
}
